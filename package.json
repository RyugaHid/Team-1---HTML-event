{
  "name": "1-team-html-event",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build-sprite": "svg-sprite -s --symbol-dest dist public/assets/icons/*.svg",
    "minify-images": "imagemin public/assets/*.{jpg,jpeg,png} --out-dir=dist/components",
    "start": "npm run build-sprite && npm run minify-images && astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@fontsource-variable/jost": "^5.1.0",
    "@fontsource/barlow": "^5.1.0",
    "astro": "^4.15.9",
    "normalize.css": "^8.0.1",
    "swiper": "^11.1.14"
  },
  "devDependencies": {
    "imagemin-cli": "^8.0.0",
    "svg-sprite": "^2.0.4"
  }
}
