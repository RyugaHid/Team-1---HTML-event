---
import Layout from "../layouts/Layout.astro";
import Header from "../components/header/Header.astro";
import Top from "../components/top/Top.astro";
import Services from "../components/services/Services.astro";
import Testemonials from "../components/testemonials/Testemonials.astro";
import Team from "../components/members/Team.astro";
import Projects from "../components/projects/Projects.astro";
import Price from "../components/price/Price.astro";
import Blog from "../components/blog/Blog.astro";

import About from "../components/about-us/About.astro";
const navLinks = [
  { name: "Home", href: "#", active: true },
  { name: "About", href: "#about", active: false },
  { name: "Projects", href: "#projects", active: false },
  { name: "Testimonials", href: "#testimonials", active: false },
  { name: "Team", href: "#team", active: false },
  { name: "Pricing", href: "#pricing", active: false },
  { name: "Block", href: "#block", active: false },
  { name: "Contact", href: "#contact", active: false },
];
---

<Layout>
  <Header navLinks={navLinks} />
  <Top />
  <Services />
  <Projects />
  <Testemonials />
  <Team />
  <Price />
  <Blog />
</Layout>
